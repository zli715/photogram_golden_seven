<div class="container">
  <div class="col-md-6">

    <h1>Add a new photo</h1>

    <form action="/create_photo">
      <div>
        <label for="source">
          Image URL
        </label>

        <input id="source" name="source">
      </div>

      <div>
        <label for="caption">
          Caption
        </label>

        <input id="caption" name="caption">
      </div>


      <button>
        Create Photo
      </button>
    </form>
  </div>
</div>
